{
  "expo": {
    "name": "Uber",
    "slug": "uber",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2f80ed"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arushsingh03.uber",
      "config": {
        "googleMapsApiKey": "AIzaSyDWmZkfE6DvnNaf3nbPjgq8uOmBMg3d7_c"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.arushsingh03.uber",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDWmZkfE6DvnNaf3nbPjgq8uOmBMg3d7_c"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://uber.com/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://uber.com/"
      },
      "eas": {
        "projectId": "6bf67b87-8820-4bb3-8817-508dcdf529c3"
      }
    },
    "owner": "abhi212004"
  }
}
